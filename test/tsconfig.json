{
  "compilerOptions": {
    "noEmit": true,
    "noImplicitAny": true,
    "noImplicitReturns": false,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "noUnusedParameters": false,
    "noFallthroughCasesInSwitch": true,
    "strictNullChecks": true,
    "target": "es5",
    "lib": ["es6", "dom"]
  }
}
